 await fetch(`${baseUrl}/worlds`);\n        const data = await res.json();\n        setWorlds(data.worlds);\n      }\n    })();\n  }, [worlds]);\n\n  useEffect(() => {\n    if (page === 2) {\n      if (heroContainer.current.children.length && heroId) {\n        heroContainer.current.childNodes.forEach(child => child.classList.remove('selected'));\n        heroContainer.current.childNodes[heroId - 1].classList.add('selected');\n      }\n    }\n  }, [heroId, page]);\n\n  useEffect(() => {\n    if (page === 1) {\n      setPageTitle('Set Your Story Name');\n    } else if (page === 2) {\n      setPageTitle('Select Your Hero');\n    } else {\n      setPageTitle('Set Your Destination');\n    }\n  }, [page])\n\n  const handleSubmit = async e => {\n    if ( !heroId || !title || !destinationTitle || !targetDate || !importance) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['There is at least one field with missing value.']\n      });\n    }\n    const data = await dispatch(createStory({ worldId, heroId, title, \n                                destinationTitle, targetDate, importance  }));\n\n    if(!data.errors) {\n      history.push(`/stories/${data.stories[data.stories.length - 1].id}`);\n    }\n  }\n\n  const handleNext = () => {\n    if ((page === 1 && !title) || (page === 2 && !heroId)) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['Please select/fill out to proceed.']\n      });\n    }\n    dispatch({ type: CLEAR_ERRORS });\n    setPage(page + 1);\n  }\n  \n  const handleBack = () => {\n    setPage(page - 1);\n  }\n\n  return ( \n    <>\n      <h2>{pageTitle}</h2>\n      {page === 1 && (\n        <div className=\"modal__field\">\n            <label for=\"title\">Story Title</label>\n            <input type=\"text\" \n              value={title} \n              name=\"title\"\n              onChange={e => setTitle(e.target.value)} />\n          </div>\n      )}\n      {page === 2 && (\n        <div className=\"modal__page-container\" ref={heroContainer}> \n          {heroes.length && heroes.map(hero => (\n            <div className=\"hero\" onClick={() => {\n              setHeroId(hero.id);\n              setWorldId(hero.worldId) }}>\n              <img src={hero.image} alt={hero.id} />\n              <p>{hero.name}</p>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 3 && (\n        <>\n          <div className=\"modal__field\">\n            <label for=\"destination-title\">Final Goal</label>\n            <input type=\"text\" \n              value={destinationTitle} \n              name=\"destination-title\"\n              onChange={e => setDestinationTitle(e.target.value)} />\n          </div>\n          <div className=\"modal__field\">\n            <label for=\"target-date\">Target Date</label>\n            <input type=\"date\" \n              value={targetDate} \n              name=\"target-date\"\n              onChange={e => setTargetDate(e.target.value)} />\n          </div>\n          <div className=\"modal__field\">\n            <label for=\"importance\">Importance</label>\n            <input type=\"number\" \n              value={importance} \n              name=\"importance\"\n              onChange={e => setImportance(e.target.value)} />\n          </div>\n          <button onClick={handleSubmit}>Create Story</button>\n        </>\n      )}\n      <div className=\"modal__button-container\">\n        <button hidden={page < 2} onClick={handleBack}>Back</button>\n        <button hidden={page > 2} onClick={handleNext}>Next</button>\n      </div>\n    </>\n  );\n}\n \nexport default StoryCreator;","/Users/yuka/Desktop/story_maker/client/src/components/NavBar.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MyStory.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/Scroll2.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewDestination.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewStoryModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/DestinationContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/Destination.js",["283","284","285"],"import React from 'react';\nimport dateFormatter from '../utils/dateFormatter';\n\nconst Destination = ({...props}) => {\n  const { id, parent_destination_id: parentDestinationId,\n        title, description, target_date: targetDate, importance } = props;\n  return ( \n    <div className=\"destination\">\n      <p className=\"destination__title title\">{title}</p>\n      <p className=\"destination__description\">{description}</p>\n      <p className=\"destination__days-left\">__ Days Left</p>\n      <p className=\"destination__target-date\">Target Date: {dateFormatter(targetDate)}</p>\n    </div>\n  );\n}\n \nexport default Destination;","/Users/yuka/Desktop/story_maker/client/src/store/actions/destinationActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/destinationReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/utils/dateFormatter.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/SideMenuField.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewMemoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/memoirReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/memoirActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MemoirContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/Memoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewHeroModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/HeroCreator.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/ActivityContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Activity.js",["286","287","288","289"],"import React, { useState, useEffect, useRef } from 'react';\nimport { useSelector } from 'react-redux';\nimport { select, line, curveCardinal, axisBottom, scaleLinear } from 'd3';\nimport randomColor from 'randomcolor';\n\nconst Activity = ({activities}) => {\n  const [lines, setLines] = useState({});\n  const [filter, setFilter] = useState(1);\n  const [colors, setColors] = useState({});\n  const heroes = useSelector(state => state.hero);\n\n  const filterActivities = () => {\n    let maxRange;\n    if (+filter === 1) {\n      maxRange = 30;\n    } else if (+filter === 3) {\n      maxRange = 90;\n    } else {\n      maxRange = 365;\n    }\n\n    const xScale = scaleLinear().domain([0, maxRange])\n                                .range([0, 800]);\n\n    const yScale = scaleLinear().domain([0, 10])\n        .range([150, 10]);\n\n    let dataLine = line().x((value, index) => xScale(index))\n    .y(yScale)\n    .curve(curveCardinal)\n\n    const currentMonth = new Date().getMonth() + 1;\n    const pathData = {};\n    const colorData = {};\n    \n    for (let heroId in activities) {\n      let combinedData = [];\n      for (let month = currentMonth + 1; month <= 12; month++) {\n        combinedData.push(...activities[heroId][month]);\n      }\n\n      for (let month = 1; month <= currentMonth; month++) {\n        combinedData.push(...activities[heroId][month]);\n      }\n      pathData[heroId] = combinedData;\n    }\n\n    if (+filter === 1) {\n      for (let heroId in activities) {\n        pathData[heroId] = dataLine(pathData[heroId].slice(-30));\n        colorData[heroId] = randomColor();\n      }\n    } else if (+filter === 3) {\n      for (let heroId in activities) {\n        pathData[heroId] = dataLine(pathData[heroId].slice(-90));\n        colorData[heroId] = randomColor();\n      }\n    } else {\n      for (let heroId in activities) {\n        pathData[heroId] = dataLine(pathData[heroId]);\n        colorData[heroId] = randomColor();\n      }\n    }\n    setLines(pathData);\n    setColors(colorData);\n  }\n\n  useEffect(() => {\n    filterActivities()\n  }, [filter])\n  \n  useEffect(() => {\n\n    if (Object.keys(lines).length !== Object.keys(activities).length) {\n      \n\n      filterActivities();\n    }\n  }, [activities, lines]);\n\n  return ( \n    <div className=\"activity\">\n      <svg>\n        {Object.entries(lines).map((line, i) => (\n          <path d={line[1]} fill='none' stroke={colors[line[0]]} />\n        ))}\n      </svg>\n      <div className=\"activity__filter-container\">\n          <div className=\"activity__category\">\n            {heroes.map(hero => (\n              <p className=\"activity__tag\">\n                <span className=\"circle\" style={{color:colors[hero.id]}}>&#9673;</span>\n                <span>{hero.name}</span>\n              </p>\n            ))}\n            <div>\n              <label for=\"activity-filter\">Filter By: </label>\n              <select name=\"activity-filter\"\n                      onChange={e => setFilter(e.target.value)}>\n                <option value={1}>Last 1 Month</option>\n                <option value={3}>Last 3 Months</option>\n                <option value={12}>Last 12 Months</option>\n              </select>\n            </div>\n          </div>\n      </div>\n    </div>\n  );\n}\nexport default Activity;","/Users/yuka/Desktop/story_maker/client/src/store/reducers/activityReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/activityAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/HeaderContainer.js",["290","291","292","293","294"],"import React, { useRef, useEffect, Suspense, useState } from 'react';\nimport { useLocation } from 'react-router-dom';\nimport { Canvas } from 'react-three-fiber';\nimport { Stars, OrbitControls } from 'drei';\nimport { useGLTF } from '@react-three/drei';\nimport NavBar from './NavBar';\n\nimport HubHeader from './hub/HubHeader';\nimport ProgressHeader from './story/ProgressHeader';\n\nconst Tavern = () => {\n  const gltf = useGLTF('/scene.gltf', true);\n\n  return (\n    <group position={[0, -10, 0]} scale={[.1, .1, .1]}>\n      <primitive object={gltf.scene} dispose={null}></primitive>\n    </group>\n  )\n}\n  \nconst HeaderContainer = () => {\n  const container = useRef();\n  const { pathname } = useLocation();\n  const [ currentGraphic, setCurrentGraphic ] = useState('myhub');\n\n  useEffect(() => {\n    if (pathname === '/my-hub') {\n      setCurrentGraphic('myhub');\n    } else {\n      setCurrentGraphic('story');\n    }\n  }, [pathname, setCurrentGraphic]);\n\n  return ( \n    <div ref={container} className=\"header__container\">\n      {currentGraphic === 'myhub' ? (\n        <HubHeader />\n      ) : <ProgressHeader />}\n      <NavBar />\n    </div>\n  );\n}\n \nexport default HeaderContainer;","/Users/yuka/Desktop/story_maker/client/src/components/three/Hero.js",["295"],"import React, { Suspense, useEffect } from 'react';\nimport { useBox } from \"@react-three/cannon\";\nimport { useTexture } from '@react-three/drei';\n\nconst Hero = ({hero, active, setActive, ...props}) => {\n  const [ref] = useBox(() => ({ mass: 1, ...props }));\n  // const texture = useTexture('https://story-maker-app.s3.amazonaws.com/bungalo.png')\n  const texture = useTexture(hero ? hero.image : '')\n\n  return ( \n    <Suspense fallback={null}>\n      <mesh \n        ref={ref}\n        >\n        <planeBufferGeometry attach=\"geometry\" />\n        <meshStandardMaterial map={texture} />\n      </mesh>\n    </Suspense>\n  );\n}\n \nexport default Hero;","/Users/yuka/Desktop/story_maker/client/src/components/three/Camera.js",["296"],"import React, { useEffect, useRef } from 'react';\nimport { a } from '@react-spring/three';\nimport { useFrame, useThree } from 'react-three-fiber';\nimport CanvasScroll from './CanvasScroll';\n\nconst Camera = (props) => {\n  const [x] = CanvasScroll([1, 100], { domTarget: props.reference });\n  const ref = useRef()\n  const { setDefaultCamera } = useThree()\n  useEffect(() => void setDefaultCamera(ref.current), [])\n  useF