;\n  const heroContainer = useRef();\n  const heroes = useSelector(state => state.hero);\n\n  const [page, setPage] = useState(1);\n  const [worlds, setWorlds] = useState([]);\n  const [worldId, setWorldId] = useState(null);\n  const [heroId, setHeroId] = useState(null);\n  const [title, setTitle] = useState('');\n  const [destinationTitle, setDestinationTitle] = useState('');\n  const [targetDate, setTargetDate] = useState('');\n  const [importance, setImportance] = useState('');\n  const [pageTitle, setPageTitle] = useState('');\n\n  useEffect(() => {\n    (async () => {\n      if (!worlds.length) {\n        const res = await fetch(`${baseUrl}/worlds`);\n        const data = await res.json();\n        setWorlds(data.worlds);\n      }\n    })();\n  }, [worlds]);\n\n  useEffect(() => {\n    if (page === 2) {\n      if (heroContainer.current.children.length && heroId) {\n        heroContainer.current.childNodes.forEach(child => child.classList.remove('selected'));\n        heroContainer.current.childNodes[heroId - 1].classList.add('selected');\n      }\n    }\n  }, [heroId, page]);\n\n  useEffect(() => {\n    if (page === 1) {\n      setPageTitle('Set Your Story Name');\n    } else if (page === 2) {\n      setPageTitle('Select Your Hero');\n    } else {\n      setPageTitle('Set Your Destination');\n    }\n  }, [page])\n\n  const handleSubmit = async e => {\n    if ( !heroId || !title || !destinationTitle || !targetDate || !importance) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['There is at least one field with missing value.']\n      });\n    }\n    const data = await dispatch(createStory({ worldId, heroId, title, \n                                destinationTitle, targetDate, importance  }));\n\n    if(!data.errors) {\n      history.push(`/stories/${data.stories[data.stories.length - 1].id}`);\n    }\n  }\n\n  const handleNext = () => {\n    if ((page === 1 && !title) || (page === 2 && !heroId)) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['Please select/fill out to proceed.']\n      });\n    }\n    dispatch({ type: CLEAR_ERRORS });\n    setPage(page + 1);\n  }\n  \n  const handleBack = () => {\n    setPage(page - 1);\n  }\n\n  return ( \n    <>\n      <h2>{pageTitle}</h2>\n      {page === 1 && (\n        <div className=\"modal__field\">\n            <label for=\"title\">Story Title</label>\n            <input type=\"text\" \n              value={title} \n              name=\"title\"\n              onChange={e => setTitle(e.target.value)} />\n          </div>\n      )}\n      {page === 2 && (\n        <div className=\"modal__page-container\" ref={heroContainer}> \n          {heroes.length && heroes.map(hero => (\n            <div className=\"hero\" onClick={() => {\n              setHeroId(hero.id);\n              setWorldId(hero.worldId) }}>\n              <img src={hero.image} alt={hero.id} />\n              <p>{hero.name}</p>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 3 && (\n        <>\n          <div className=\"modal__field\">\n            <label for=\"destination-title\">Final Goal</label>\n            <input type=\"text\" \n              value={destinationTitle} \n              name=\"destination-title\"\n              onChange={e => setDestinationTitle(e.target.value)} />\n          </div>\n          <div className=\"modal__field\">\n            <label for=\"target-date\">Target Date</label>\n            <input type=\"date\" \n              value={targetDate} \n              name=\"target-date\"\n              onChange={e => setTargetDate(e.target.value)} />\n          </div>\n          <div className=\"modal__field\">\n            <label for=\"importance\">Importance</label>\n            <input type=\"number\" \n              value={importance} \n              name=\"importance\"\n              onChange={e => setImportance(e.target.value)} />\n          </div>\n          <button onClick={handleSubmit}>Create Story</button>\n        </>\n      )}\n      <div className=\"modal__button-container\">\n        <button hidden={page < 2} onClick={handleBack}>Back</button>\n        <button hidden={page > 2} onClick={handleNext}>Next</button>\n      </div>\n    </>\n  );\n}\n \nexport default StoryCreator;","/Users/yuka/Desktop/story_maker/client/src/components/NavBar.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MyStory.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/Scroll2.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewDestination.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewStoryModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/DestinationContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/Destination.js",["292","293","294"],"import React from 'react';\nimport dateFormatter from '../utils/dateFormatter';\n\nconst Destination = ({...props}) => {\n  const { id, parent_destination_id: parentDestinationId,\n        title, description, target_date: targetDate, importance } = props;\n  return ( \n    <div className=\"destination\">\n      <p className=\"destination__title title\">{title}</p>\n      <p className=\"destination__description\">{description}</p>\n      <p className=\"destination__days-left\">__ Days Left</p>\n      <p className=\"destination__target-date\">Target Date: {dateFormatter(targetDate)}</p>\n    </div>\n  );\n}\n \nexport default Destination;","/Users/yuka/Desktop/story_maker/client/src/store/actions/destinationActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/destinationReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/utils/dateFormatter.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/SideMenuField.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewMemoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/memoirReducer.js",[],"/Users/yuka/Desktop/story_maker/client