sedDeprecatedRules":"118"},{"filePath":"230","messages":"231","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"232","usedDeprecatedRules":"118"},{"filePath":"233","messages":"234","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"118"},{"filePath":"235","messages":"236","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"118"},"/Users/yuka/Desktop/story_maker/client/src/index.js",[],["237","238"],"/Users/yuka/Desktop/story_maker/client/src/App.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/configureStore.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/rootReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/userReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/errorReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/splash/Splash.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/splash/LoginForm.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/InputField.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/userAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/config.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/splash/SignupForm.js",["239"],"import React, { useState } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { useDispatch } from 'react-redux';\nimport InputField from '../InputField';\nimport { createUser } from '../../store/actions/userAction';\nimport { LOAD_ERRORS } from '../../store/reducers/errorReducer';\n\nconst SignupForm = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const [username, setUsername] = useState('');\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n  const [confirmPassword, setConfirmPassword] = useState('');\n\n  const HandleSignup = async e => {\n    e.preventDefault();\n    const data = await dispatch(createUser({ username, email, password, confirmPassword }));\n    \n    if(!data.errors) {\n      history.push('/my-hub');\n    }\n  };\n\n  return ( \n    <>\n      <h4>Sign Up</h4>\n      <form onSubmit={HandleSignup}>\n        <InputField \n          type=\"text\" \n          placeholder=\"Username\"\n          currentState={username}\n          updateState={setUsername}\n        />\n        <InputField \n          type=\"email\" \n          placeholder=\"Email\"\n          currentState={email}\n          updateState={setEmail}\n        />\n        <InputField \n          type=\"password\" \n          placeholder=\"Password\"\n          currentState={password}\n          updateState={setPassword}\n        />\n        <InputField \n          type=\"password\" \n          placeholder=\"Confirm password\"\n          currentState={confirmPassword}\n          updateState={setConfirmPassword}\n        />\n        <input \n          type='submit' \n          value=\"Submit\" \n        />\n      </form>\n    </>\n  );\n}\n \nexport default SignupForm;","/Users/yuka/Desktop/story_maker/client/src/components/hub/MyHub.js",["240","241","242","243"],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/tokenReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/ActivityScroll.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/DestinationScroll.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/HeroContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Hero.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/heroReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/heroAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/StoryContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Story.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/storyAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/storyReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewAdventure.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/ModalContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/StoryCreator.js",["244"],"import React, { useState, useEffect, useRef } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { useHistory } from 'react-router-dom';\nimport { baseUrl } from '../../config';\nimport { LOAD_ERRORS, CLEAR_ERRORS } from '../../store/reducers/errorReducer';\nimport { createStory } from '../../store/actions/storyAction';\n\nconst StoryCreator = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const worldContainer = useRef();\n  const heroContainer = useRef();\n  const heroes = useSelector(state => state.hero);\n\n  const [page, setPage] = useState(1);\n  const [worlds, setWorlds] = useState([]);\n  const [worldId, setWorldId] = useState(null);\n  const [heroId, setHeroId] = useState(null);\n  const [title, setTitle] = useState('');\n  const [destinationTitle, setDestinationTitle] = useState('');\n  const [targetDate, setTargetDate] = useState('');\n  const [importance, setImportance] = useState('');\n  const [pageTitle, setPageTitle] = useState('');\n\n  useEffect(() => {\n    (async () => {\n      if (!worlds.length) {\n        const res = await fetch(`${baseUrl}/worlds`);\n        const data = await res.json();\n        setWorlds(data.worlds);\n      }\n    })();\n  }, [worlds]);\n\n  useEffect(() => {\n    if (page === 2) {\n      if (heroContainer.current.children.length && heroId) {\n        heroContainer.current.childNodes.forEach(child => child.classList.remove('selected'));\n        heroContainer.current.childNodes[heroId - 1].classList.add('selected');\n      }\n    }\n  }, [heroId, page]);\n\n  useEffect(() => {\n    if (page === 1) {\n      setPageTitle('Set Your Story Name');\n    } else if (page === 2) {\n      setPageTitle('Select Your Hero');\n    } else {\n      setPageTitle('Set Your Destination');\n    }\n  }, [page])\n\n  const handleSubmit = async e => {\n    if ( !heroId || !title || !destinationTitle || !targetDate || !importance) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['There is at least one field with missing value.']\n      });\n    }\n    const data = await dispatch(createStory({ worldId, heroId, title, \n                                destinationTitle, targetDate, importance  }));\n\n    if(!data.errors) {\n      history.push(`/stories/${data.stories[data.stories.length - 1].id}`);\n    }\n  }\n\n  const handleNext = () => {\n    if ((page === 1 && !title) || (page === 2 && !heroId)) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['Please select/fill out to proceed.']\n      });\n    }\n    dispatch({ type: CLEAR_ERRORS });\n    setPage(page + 1);\n  }\n  \n  const handleBack = () => {\n    setPage(page - 1);\n  }\n\n  return ( \n    <>\n      <h2>{pageTitle}</h2>\n      {page === 1 && (\n        <div>\n            <label for=\"title\">Story Title</label>\n            <input type=\"text\" \n              value={title} \n              name=\"title\"\n              onChange={e => setTitle(e.target.value)} />\n          </div>\n      )}\n      {page === 2 && (\n        <div className=\"modal__page-container\" ref={heroContainer}> \n          {heroes.length && heroes.map(hero => (\n            <div className=\"hero\" onClick={() => {\n              setHeroId(hero.id);\n              setWorldId(hero.worldId) }}>\n              <img src={hero.image} alt={hero.id} />\n              <p>{hero.name}</p>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 3 && (\n        <>\n          <div>\n            <label for=\"destination-title\">Final Goal</label>\n            <input type=\"text\" \n              value={destinationTitle} \n              name=\"destination-title\"\n              onChange={e => setDestinationTitle(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"target-date\">Target Date</label>\n            <input type=\"date\" \n              value={targetDate} \n              name=\"target-date\"\n              onChange={e => setTargetDate(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"importance\">Importance</label>\n            <input type=\"number\" \n              value={importance} \n              name=\"importance\"\n              onChange={e => setImportance(e.target.value)} />\n          </div>\n          <button onClick={handleSubmit}>Create Story</button>\n        </>\n      )}\n      <button hidden={page < 2} onClick={handleBack}>Back</button>\n      <button hidden={page > 2} onClick={handleNext}>Next</button>\n    </>\n  );\n}\n \nexport default StoryCreator;","/Users/yuka/Desktop/story_maker/client/src/components/Header.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/NavBar.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MyStory.js",["245"],"import React, { useState, useEffect } from 'react';\nimport { useDispatch } from 'react-redux';\nimport { useParams } from 'react-router-dom';\nimport Scroll2 from '../svg/Scroll2';\nimport NewDestination from './NewDestination';\nimport DestinationContainer from '../DestinationContainer';\nimport SideMenuField from './SideMenuField';\nimport ProgressContainer from './ProgressContainer';\nimport MemoirContainer from './MemoirContainer';\nimport { getStory } from '../../store/actions/storyAction';\n\nconst MyStory = () => {\n  const dispatch = useDispatch();\n  const { id } = useParams();\n  const [currentDisplay, setCurrentDisplay] = useState(<DestinationContainer />);\n  const [currentTitle, setCurrentTitle] = useState(\"Upcoming destinations\");\n\n  useEffect(() => {\n    dispatch(getStory(id));\n  }, []);\n\n  return (  \n    <div className=\"mystory\">\n       <div className=\"mystory__progress\">\n         <ProgressContainer />\n       </div>\n       <div className=\"mystory__main\">\n         <Scroll2 text={currentTitle} className=\"mystory__scroll\" \n                  width=\"40rem\" fontSize=\".5rem\" />\n         <div className=\"mystory__container\">\n          {currentDisplay}\n         </div>\n       </div>\n       <div className=\"mystory__sidebar-left\">\n       <SideMenuField>\n        <div onClick={() => {\n          setCurrentDisplay(<DestinationContainer />);\n          setCurrentTitle(\"Upcoming Destinations\");\n        }}>\n          View Upcoming Destinations\n        </div>\n       </SideMenuField>\n       <SideMenuField>\n       <div onClick={() => {\n          setCurrentDisplay(<MemoirContainer />);\n          setCurrentTitle(\"View Memoirs\");\n        }}>\n          Memoirs\n        </div>\n       </SideMenuField>\n\n       </div>\n       <div className=\"mystory__sidebar-right\">\n        <NewDestination />\n       </div>\n    </div>\n  );\n}\n \nexport default MyStory;","/Users/yuka/Desktop/story_maker/client/src/components/svg/Scroll2.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewDestination.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewStoryModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/DestinationContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/Destination.js",["246","247","248"],"/Users/yuka/Desktop/story_maker/client/src/store/actions/destinationActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/destinationReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/utils/dateFormatter.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/SideMenuField.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewMemoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/ProgressContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reduce