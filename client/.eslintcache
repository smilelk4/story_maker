ra } = useThree()\n  useEffect(() => void setDefaultCamera(ref.current), [])\n  useFrame(() => ref.current.updateMatrixWorld())\n\n  return ( \n    <a.perspectiveCamera ref={ref} {...props} \n          fov=\"10\" position-x={x.to((x) => (x / 500) * 25)}/>\n   );\n}\n \nexport default Camera;","/Users/yuka/Desktop/story_maker/client/src/components/three/CanvasScroll.js",["253"],"import React, { useEffect, useCallback } from 'react';\nimport { useGesture } from 'react-use-gesture';\nimport { useSpring, config } from '@react-spring/core';\nimport clamp from \"lodash/clamp\";\n\nconst CanvasScroll = (bounds, props) => {\n  const [{ x }, set] = useSpring(() => ({ x: 0, config: config.slow }));\n  const fn = useCallback(\n    ({ xy: [cx, ], previous: [, px], memo = x.get() }) => {\n      const newX = clamp(memo + cx - px, ...bounds);\n      set({ x: newX });\n      return newX;\n    },\n    [bounds, x, set]\n  );\n  const bind = useGesture({ onWheel: fn, onDrag: fn }, props);\n  useEffect(() => props && props.domTarget && bind(), [props, bind]);\n  return [x, bind];\n}\n \nexport default CanvasScroll;","/Users/yuka/Desktop/story_maker/client/src/components/three/Plane.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/three/Node.js",[],{"ruleId":"254","replacedBy":"255"},{"ruleId":"256","replacedBy":"257"},{"ruleId":"258","severity":1,"message":"259","line":6,"column":10,"nodeType":"260","messageId":"261","endLine":6,"endColumn":21},{"ruleId":"258","severity":1,"message":"262","line":1,"column":17,"nodeType":"260","messageId":"261","endLine":1,"endColumn":25},{"ruleId":"258","severity":1,"message":"263","line":1,"column":27,"nodeType":"260","messageId":"261","endLine":1,"endColumn":36},{"ruleId":"258","severity":1,"message":"264","line":1,"column":38,"nodeType":"260","messageId":"261","endLine":1,"endColumn":44},{"ruleId":"258","severity":1,"message":"265","line":2,"column":10,"nodeType":"260","messageId":"261","endLine":2,"endColumn":17},{"ruleId":"258","sever