 [destinationTitle, setDestinationTitle] = useState('');\n  const [targetDate, setTargetDate] = useState('');\n  const [importance, setImportance] = useState('');\n  const [pageTitle, setPageTitle] = useState('');\n\n  useEffect(() => {\n    (async () => {\n      if (!worlds.length) {\n        const res = await fetch(`${baseUrl}/worlds`);\n        const data = await res.json();\n        setWorlds(data.worlds);\n      }\n    })();\n  }, [worlds]);\n\n  useEffect(() => {\n    if (page === 2) {\n      if (heroContainer.current.children.length && heroId) {\n        heroContainer.current.childNodes.forEach(child => child.classList.remove('selected'));\n        heroContainer.current.childNodes[heroId - 1].classList.add('selected');\n      }\n    }\n  }, [heroId, page]);\n\n  useEffect(() => {\n    if (page === 1) {\n      setPageTitle('Set Your Story Name');\n    } else if (page === 2) {\n      setPageTitle('Select Your Hero');\n    } else {\n      setPageTitle('Set Your Destination');\n    }\n  }, [page])\n\n  const handleSubmit = async e => {\n    if ( !heroId || !title || !destinationTitle || !targetDate || !importance) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['There is at least one field with missing value.']\n      });\n    }\n    const data = await dispatch(createStory({ worldId, heroId, title, \n                                destinationTitle, targetDate, importance  }));\n\n    if(!data.errors) {\n      history.push(`/stories/${data.stories[data.stories.length - 1].id}`);\n    }\n  }\n\n  const handleNext = () => {\n    if ((page === 1 && !title) || (page === 2 && !heroId)) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['Please select/fill out to proceed.']\n      });\n    }\n    dispatch({ type: CLEAR_ERRORS });\n    setPage(page + 1);\n  }\n  \n  const handleBack = () => {\n    setPage(page - 1);\n  }\n\n  return ( \n    <>\n      <h2>{pageTitle}</h2>\n      {page === 1 && (\n        <div>\n            <label for=\"title\">Story Title</label>\n            <input type=\"text\" \n              value={title} \n              name=\"title\"\n              onChange={e => setTitle(e.target.value)} />\n          </div>\n      )}\n      {page === 2 && (\n        <div className=\"modal__page-container\" ref={heroContainer}> \n          {heroes.length && heroes.map(hero => (\n            <div className=\"hero\" onClick={() => {\n              setHeroId(hero.id);\n              setWorldId(hero.worldId) }}>\n              <img src={hero.image} alt={hero.id} />\n              <p>{hero.name}</p>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 3 && (\n        <>\n          <div>\n            <label for=\"destination-title\">Final Goal</label>\n            <input type=\"text\" \n              value={destinationTitle} \n              name=\"destination-title\"\n              onChange={e => setDestinationTitle(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"target-date\">Target Date</label>\n            <input type=\"date\" \n              value={targetDate} \n              name=\"target-date\"\n              onChange={e => setTargetDate(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"importance\">Importance</label>\n            <input type=\"number\" \n              value={importance} \n              name=\"importance\"\n              onChange={e => setImportance(e.target.value)} />\n          </div>\n          <button onClick={handleSubmit}>Create Story</button>\n        </>\n      )}\n      <button hidden={page < 2} onClick={handleBack}>Back</button>\n      <button hidden={page > 2} onClick={handleNext}>Next</button>\n    </>\n  );\n}\n \nexport default StoryCreator;","/Users/yuka/Desktop/story_maker/client/src/components/NavBar.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MyStory.js",["260"],"import React, { useState, useEffect } from 'react';\nimport { useDispatch } from 'react-redux';\nimport { useParams } from 'react-router-dom';\nimport Scroll2 from '../svg/Scroll2';\nimport NewDestination from './NewDestination';\nimport DestinationContainer from '../DestinationContainer';\nimport SideMenuField from './SideMenuField';\nimport ProgressContainer from './ProgressContainer';\nimport MemoirContainer from './MemoirContainer';\nimport { getStory } from '../../store/actions/storyAction';\n\nconst MyStory = () => {\n  const dispatch = useDispatch();\n  const { id } = useParams();\n  const [currentDisplay, setCurrentDisplay] = useState(<DestinationContainer />);\n  const [currentTitle, setCurrentTitle] = useState(\"Upcoming destinations\");\n\n  useEffect(() => {\n    dispatch(getStory(id));\n  }, []);\n\n  return (  \n    <div className=\"mystory\">\n       <div className=\"mystory__progress\">\n         <ProgressContainer />\n       </div>\n       <div className=\"mystory__main\">\n         <Scroll2 text={currentTitle} className=\"mystory__scroll\" \n                  width=\"40rem\" fontSize=\".5rem\" />\n         <div className=\"mystory__container\">\n          {currentDisplay}\n         </div>\n       </div>\n       <div className=\"mystory__sidebar-left\">\n       <SideMenuField>\n        <div onClick={() => {\n          setCurrentDisplay(<DestinationContainer />);\n          setCurrentTitle(\"Upcoming Destinations\");\n        }}>\n          View Upcoming Destinations\n        </div>\n       </SideMenuField>\n       <SideMenuField>\n       <div onClick={() => {\n          setCurrentDisplay(<MemoirContainer />);\n          setCurrentTitle(\"View Memoirs\");\n        }}>\n          Memoirs\n        </div>\n       </SideMenuField>\n\n       </div>\n       <div className=\"mystory__sidebar-right\">\n        <NewDestination />\n       </div>\n    </div>\n  );\n}\n \nexport default MyStory;","/Users/yuka/Desktop/story_maker/client/src/components/svg/Scroll2.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewDestination.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewStoryModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/DestinationContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/Destination.js",["261","262","263"],"import React from 'react';\nimport dateFormatter from '../utils/dateFormatter';\n\nconst Destination = ({...props}) => {\n  const { id, parent_destination_id: parentDestinationId,\n        title, description, target_date: targetDate, importance } = props;\n  return ( \n    <div className=\"destination\">\n      <p className=\"destination__title title\">{title}</p>\n      <p className=\"destination__description\">{description}</p>\n      <p className=\"destination__days-left\">__ Days Left</p>\n      <p className=\"destination__target-date\">Target Date: {dateFormatter(targetDate)}</p>\n    </div>\n  );\n}\n \nexport default Destination;","/Users/yuka/Desktop/story_maker/client/src/store/actions/destinationActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/destinationReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/utils/dateFormatter.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/SideMenuField.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewMemoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/ProgressContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/memoirReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/memoirActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MemoirContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/Memoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewHeroModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/HeroCreator.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/ActivityContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Activity.js",["264","265"],"import React, { useState, useEffect, useRef } from 'react';\nimport { select, line, curveCardinal, axisBottom, scaleLinear } from 'd3';\n\nconst Activity = ({activities}) => {\n  const [points, setPoints] = useState({});\n  const lineColors = {\n    0: 'red',\n    1: 'blue'\n  }\n\n  // const points = [];\n\n  useEffect(() => {\n    if (Object.keys(points).length !== Object.keys(activities).length) {\n\n      const xScale = scaleLinear().domain([0, 31])\n                                  .range([0, 150]);\n    \n      const yScale = scaleLinear().domain([0, 10])\n                                  .range([150, 0]);\n  \n      let dataLine = line().x((value, index) => xScale(index))\n                            .y(yScale)\n                            .curve(curveCardinal)\n\n      for (let heroId in activities) {\n        let combinedMonthData = [];\n        for (let monthData in activities[heroId]) {\n          combinedMonthData.push(...activities[heroId][monthData]);\n        }\n        setPoints({\n          ...points,\n          [heroId]: dataLine(combinedMonthData)\n        });\n      }\n    }\n  }, [activities, points]);\n\n  return ( \n    <div className=\"activity\">\n      <svg>\n        {Object.values(points).map((point, i) => (\n           <path d={point} fill='none' stroke={lineColors[i]} />\n        ))}\n      </svg>\n      <div className=\"activity__filter-container\">\n          <div className=\"activity__category\">\n            <label for=\"filter\">Filter By: </label>\n            <select>\n              <option>Last 1 Month</option>\n              <option>Last 3 Months</option>\n              <option>Last 12 Months</option>\n            </select>\n          </div>\n      </div>\n    </div>\n  );\n}\nexport default Activity;","/Users/yuka/Desktop/story_maker/client/src/store/reducers/activityReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/activityAction.js",[],"/Users/yuka/