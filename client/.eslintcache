 \n  const gltf = useGLTF('/gltf/trees/scene.gltf', true);\n  // const texture = useTexture('https://story-maker-app.s3.amazonaws.com/tree-2.png')\n  // const texture = useTexture(destination.parent_destination_id ? 'https://story-maker-app.s3.amazonaws.com/tree-2.png' : 'https://story-maker-app.s3.amazonaws.com/castle.png')\n\n  return ( \n    <Suspense fallback={null}>\n      {/* {destination.parent_destination_id ? ( */}\n        <mesh \n        scale={[.35, .35, .35]}\n        ref={ref}\n        onClick={() => setActive(destination.id)}\n        onPointerOver={() => setHovered(true)}\n        onPointerOut={() => setHovered(false)}\n        >\n          <icosahedronGeometry attach=\"geometry\"/>\n          <meshStandardMaterial color=\"orange\"/>\n        </mesh>\n      {/* ) : (\n        <group scale={[.05, .05, .05]}>\n        <primitive ref={ref} object={gltf.scene} dispose={null}>\n          <planeBufferGeometry attach=\"geometry\" />\n        </primitive>\n      </group>\n      )} */}\n    </Suspense>\n  );\n}\n \nexport default Node;","/Users/yuka/Desktop/NEWstory_maker/client/src/components/ModalContainer.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/hub/HeroCreator.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/utils/dateFormatter.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/hub/StoryCreator.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/three/CanvasScroll.js",["475"],"import React, { useEffect, useCallback } from 'react';\nimport { useGesture } from 'react-use-gesture';\nimport { useSpring, config } from '@react-spring/core';\nimport clamp from \"lodash/clamp\";\n\nconst CanvasScroll = (bounds, props) => {\n  const [{ x }, set] = useSpring(() => ({ x: 0, config: config.slow }));\n  const fn = useCallback(\n    ({ xy: [cx, ], previous: [, px], memo = x.get() }) => {\n      const newX = clamp(memo + cx - px, ...bounds);\n      set({ x: newX });\n      return newX;\n    },\n    [bounds, x, set]\n  );\n  const bind = useGesture({ onWheel: fn, onDrag: fn }, props);\n  useEffect(() => props && props.domTarget && bind(), [props, bind]);\n  return [x, bind];\n}\n \nexport default CanvasScroll;","/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/SideMenuField.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/StoryHomeContainer.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/HistoryContainer.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/NewDestination.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/MonsterContainer.js",["476"],"import React, { useEffect } from 'react'\nimport NewMonster from './NewMonster';\nimport PageAnimationWrapper from '../PageAnimationWrapper';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { useParams } from 'react-router-dom';\nimport { getMonsters } from '../../store/actions/monsterAction';\nimport Monster from './Monster';\n\nconst MonsterContainer = () => {\n  const dispatch = useDispatch();\n  const { id } = useParams();\n\n  const monsters = useSelector(state => state.monster);\n\n  useEffect(() => {\n    if (!monsters.length) {\n      dispatch(getMonsters(id));\n    }\n  },[id, dispatch]);\n\n\n  return ( \n    <PageAnimationWrapper>\n      <NewMonster />\n      <div className=\"monster__container\">\n        {monsters.map(monster => <Monster {...monster} />)}\n      </div>\n    </PageAnimationWrapper>\n  );\n}\n \nexport default MonsterContainer;","/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/MemoirContainer.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/FightMonster.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/DailyTaskContainer.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/svg/Frame.js",[],"/Users/yuka/Desktop/NEWstory_maker/client/src/components/story/OverviewContainer.js",["477","478"],"import React, { useEffect, useStat