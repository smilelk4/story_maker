StoryModal.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/FightMonsterModal.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/hub/Hero.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/NewMonster.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/NewDailyTask.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/DailyTask.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/animation/navHamburger.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/svg/SwordAndShield.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/ModalContainer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/hub/HeroCreator.js",["407"],"import React, { useState, useEffect, useRef } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { baseUrl } from '../../config';\nimport { loadErrors } from '../../store/reducers/errorReducer';\nimport { createHero } from '../../store/actions/heroAction';\n\nconst HeroCreator = ({setIsModalOpen}) => {\n  const dispatch = useDispatch();\n  const worldContainer = useRef();\n  const heroContainer = useRef();\n  const userId = useSelector(state => state.user.id);\n  const errors = useSelector(state => state.errors);\n\n  const [page, setPage] = useState(1);\n  const [heroes, setHeroes] = useState([]);\n  const [worlds, setWorlds] = useState([]);\n  const [worldId, setWorldId] = useState(null);\n  const [heroId, setHeroId] = useState(null);\n  const [name, setName] = useState('');\n  const [pageTitle, setPageTitle] = useState('');\n\n  useEffect(() => {\n    (async () => {\n      if (heroes && !heroes.length) {\n        const res = await fetch(`${baseUrl}/heroes`, {\n          headers : { \n            'Content-Type': 'application/json',\n            'Accept': 'application/json'\n           }\n        });\n        const data = await res.json();\n        setHeroes(data.heroImages);\n      }\n    })();\n  }, [heroes]);\n\n  useEffect(() => {\n    (async () => {\n      if (!worlds.length) {\n        const res = await fetch(`${baseUrl}/worlds`, {\n          headers : { \n            'Content-Type': 'application/json',\n            'Accept': 'application/json'\n           }\n        });\n        const data = await res.json();\n        setWorlds(data.worlds);\n      }\n    })();\n  }, [worlds]);\n\n  useEffect(() => {\n    if (page === 2) {\n      if (worldContainer.current.children.length && worldId) {\n        worldContainer.current.childNodes.forEach(child => {\n          child.classList.remove('modal__content--selected');\n        });\n      }\n    }\n  }, [worldId, page])\n\n  useEffect(() => {\n    if (page === 3) {\n      if (heroContainer.current.children.length && worldId) {\n        worldContainer.current.childNodes.forEach(child => {\n          child.classList.remove('modal__content--selected');\n        });\n      }\n    }\n  }, [heroId, page]);\n\n  useEffect(() => {\n    if (page === 1) {\n      setPageTitle('Select Your Hero Name');\n    } else if (page === 2) {\n      setPageTitle('Select Your World');\n    } else {\n      setPageTitle('Select Your Hero');\n    }\n  }, [page])\n\n  const handleSubmit = async e => {\n    if (!worldId || !heroId || !name) {\n      return dispatch(loadErrors(['There is at least one field with missing value.']));\n    }\n    const data = await dispatch(createHero({ userId, worldId, name, heroId }));\n\n    if (!data.errors) {\n      setIsModalOpen(false);\n    }\n  }\n\n  const handleNext = () => {\n    if ((page === 1 && !name) || (page === 2 && !worldId)) {\n      return dispatch(loadErrors(['Please select to proceed.']));\n    }\n    setPage(page + 1);\n  }\n  \n  const handleBack = () => {\n    setPage(page - 1);\n  }\n\n  return ( \n    <>\n      <h2 className=\"title\">{pageTitle}</h2>\n      {page === 1 && (\n        <>\n        <div className=\"modal__field\">\n          <label for=\"name\">Hero Name</label>\n          <input type=\"text\" \n              value={name} \n              name=\"name\"\n              onChange={e => setName(e.target.value)} />\n          </div>\n        </>\n      )}\n      {page === 2 && (\n        <div className=\"modal__page-container\" ref={worldContainer}> \n          {worlds.length && worlds.map(world => (\n            <div className=\"world\" onClick={e => {\n              setWorldId(world.id);\n              e.target.classList.add('modal__content--selected');\n              }}>\n              <div className=\"world__name\">{world.name}</div>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 3 && (\n        <>\n        <div className=\"modal__page-container\" ref={heroContainer}> \n          {heroes && heroes.length && heroes.map(hero => (\n            <div className=\"hero\" onClick={e => {\n              setHeroId(hero.id);\n              e.target.classList.add('modal__content--selected');\n              }}>\n              <img src={hero.image_url} alt={hero.id} />\n              <p>{hero.name}</p>\n            </div>\n          ))}\n        </div>\n        <button onClick={handleSubmit}>Create Hero</button>\n        </>\n      )}\n      <div className=\"modal__errors-container\">\n        {errors.map(error => (\n          <div key={error.message}>{error}</div>\n        ))}\n      </div>\n      <div className=\"modal__button-container\">\n        <button disabled={page < 2} onClick={handleBack}>Back</button>\n        <button disabled={page > 2} onClick={handleNext}>Next</button>\n      </div>\n    </>\n  );\n}\n \nexport default HeroCreator;","/Users/yuka/Desktop/Projects/story_maker/client/src/animation/plusOne.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/svg/Shield.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/three/CanvasScroll.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/Mo