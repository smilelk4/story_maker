ries) {\n      if (story) {\n        // debugger\n        dispatch(getUpcomingDestinations(story.id));\n      }\n    }\n  }, [stories, dispatch]);\n\n  const onCompleted = async (destinationId) => {\n    const data = await dispatch(completeDestination(destinationId));\n    if (data.errors) return;\n  };\n\n  return ( \n    <PageAnimationWrapper>\n      <div className=\"destination__container\">\n        {destinations.map(destination => (\n          <>\n            {destinations[0] !== destination && <Line /> }\n            <Destination handleClick={onCompleted} {...destination} />\n          </>\n        ))}\n      </div>\n    </PageAnimationWrapper>\n  );\n}\n \nexport default DestinationContainer;","/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/SideMenuField.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/splash/AnimatedSection.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/MonsterContainer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/DailyTaskContainer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/HistoryContainer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/actions/storyAction.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/OverviewContainer.js",["471","472","473","474","475","476","477"],"import React, { useEffect, useState } from 'react'\nimport moment from 'moment';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { useParams } from 'react-router-dom';\nimport { getMemoirs } from '../../store/actions/memoirActions';\nimport { getDailyTasks } from '../../store/actions/dailyTaskAction';\nimport { getProgress } from '../../store/actions/progressAction';\nimport Overview from './Overview';\n\nconst OverviewContainer = () => {\n  const dispatch = useDispatch();\n  const tasks = useSelector(state => state.task);\n  const destinations = useSelector(state => state.destination);\n  const progress = useSelector(state => state.progress);\n  const [uncompletedTasks, setUncompletedTasks] = useState([]);\n  const [overdueDestinations, setOverdueDestinations] = useState([]);\n  const { id } = useParams();\n  const [currentStoryId, setCurrentStoryId] = useState(id);\n  const today = moment().format(\"MM-DD-YYYY\");\n\n  // useEffect(() => {\n  //   dispatch(getMemoirs(id));\n  //   dispatch(getDailyTasks(id));\n  //   // dispatch(getProgress(id));\n  // },[id, dispatch]);\n\n  useEffect(() => {\n    const uncompletedTasks = tasks.filter(task => (\n              (moment(task.last_accomplished).format(\"MM-DD-YYYY\")) < today) ||\n                !task.last_accomplished);\n    setUncompletedTasks(uncompletedTasks);\n  },[id, tasks, dispatch]);\n\n  useEffect(() => {\n    const overdueDestinations = destinations.filter(destination => (\n              moment(destination.target_date).isBefore(today, 'days')));\n    setOverdueDestinations(overdueDestinations);\n  },[destinations]);\n\n  return ( \n    <div className=\"overview__container\">\n      <Overview progress={progress}\n                uncompletedTasks={uncompletedTasks}\n                overdueDestinations={overdueDestinations} />\n    </div>\n  );\n}\n \nexport default OverviewContainer;","/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/FightMonster.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/hub/NewAdventure.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/hub/StoryContainer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/hub/HeroContainer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/errorReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/MemoirContainer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/splash/SignupForm.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/hub/Greeting.j