ield \n          type=\"email\" \n          placeholder=\"Email\"\n          currentState={email}\n          updateState={setEmail}\n        />\n        <InputField \n          type=\"password\" \n          placeholder=\"Password\"\n          currentState={password}\n          updateState={setPassword}\n        />\n        <InputField \n          type=\"password\" \n          placeholder=\"Confirm password\"\n          currentState={confirmPassword}\n          updateState={setConfirmPassword}\n        />\n        <input \n          type='submit' \n          value=\"Submit\" \n        />\n      </form>\n    </>\n  );\n}\n \nexport default SignupForm;","/Users/yuka/Desktop/story_maker/client/src/components/hub/MyHub.js",["148"],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/tokenReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/ActivityScroll.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/DestinationScroll.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/StoryScroll.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/DestinationContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Destination.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/HeroContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Hero.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/heroReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/heroAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/StoryContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Story.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/storyAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/storyReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewAdventure.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/Modal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/ModalContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/StoryCreator.js",["149","150"],"import React, { useState, useEffect, useRef } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { useHistory } from 'react-router-dom';\nimport { baseUrl } from '../../config';\nimport { LOAD_ERRORS, CLEAR_ERRORS } from '../../store/reducers/errorReducer';\nimport { createStory } from '../../store/actions/storyAction';\n\nconst StoryCreator = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const worldContainer = useRef();\n  const heroContainer = useRef();\n  const userId = useSelector(state => state.user);\n  const heroes = useSelector(state => state.hero);\n  const stories = useSelector(state => state.story);\n\n  const [page, setPage] = useState(1);\n  const [worlds, setWorlds] = useState([]);\n  const [worldId, setWorldId] = useState(null);\n  const [heroId, setHeroId] = useState(null);\n  const [title, setTitle] = useState('');\n  const [destinationTitle, setDestinationTitle] = useState('');\n  const [targetDate, setTargetDate] = useState('');\n  const [importance, setImportance] = useState('');\n  const [pageTitle, setPageTitle] = useState('');\n\n  useEffect(() => {\n    (async () => {\n      if (!worlds.length) {\n        const res = await fetch(`${baseUrl}/worlds`);\n        const data = await res.json();\n        setWorlds(data.worlds);\n      }\n    })();\n  }, [worlds]);\n\n  useEffect(() => {\n    if (page === 1) {\n      if (worldContainer.current.children.length && worldId) {\n        worldContainer.current.childNodes.forEach(child => child.classList.remove('selected'));\n        worldContainer.current.childNodes[worldId - 1].classList.add('selected');\n      }\n    }\n  }, [worldId, page])\n\n  useEffect(() => {\n    if (page === 2) {\n      if (heroContainer.current.children.length && heroId) {\n        heroContainer.current.childNodes.forEach(child => child.classList.remove('selected'));\n        heroContainer.current.childNodes[heroId - 1].classList.add('selected');\n      }\n    }\n  }, [heroId, page]);\n\n  useEffect(() => {\n    if (page === 1) {\n      setPageTitle('Select Your World');\n    } else if (page === 2) {\n      setPageTitle('Select Your Hero');\n    } else {\n      setPageTitle('Decide on Your Story');\n    }\n  }, [page])\n\n  const handleSubmit = async e => {\n    if (!worldId || !heroId || !title || !destinationTitle || !targetDate || !importance) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['There is a field with missing values.']\n      });\n    }\n    const data = await dispatch(createStory({ worldId, heroId, title, \n                                destinationTitle, targetDate, importance  }));\n\n    if(!data.errors) {\n      history.push(`/stories/${data.stories[data.stories.length - 1].id}`);\n    }\n  }\n\n  const handleNext = () => {\n    if ((page === 1 && !worldId) || (page === 2 && !heroId)) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['Please select to proceed.']\n      });\n    }\n    dispatch({ type: CLEAR_ERRORS });\n    setPage(page + 1);\n  }\n  \n  const handleBack = () => {\n    setPage(page - 1);\n  }\n\n  return ( \n    <>\n      <h2>{pageTitle}</h2>\n      {page === 1 && (\n        <div className=\"modal__page-container\" ref={worldContainer}> \n          {worlds.length && worlds.map(world => (\n            <div className=\"world\" onClick={() => setWorldId(world.id)}>\n              <div className=\"world__name\">{world.name}</div>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 2 && (\n        <div className=\"modal__page-container\" ref={heroContainer}> \n          {heroes.length && heroes.map(hero => (\n            <div className=\"hero\" onClick={() => setHeroId(hero.id)}>\n              <img src={hero.image} alt={hero.id} />\n              <p>{hero.name}</p>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 3 && (\n        <>\n          <div>\n            <label for=\"title\">Story Title</label>\n            <input type=\"text\" \n              value={title} \n              name=\"title\"\n              onChange={e => setTitle(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"destination-title\">Final Goal</label>\n            <input type=\"text\" \n              value={destinationTitle} \n              name=\"destination-title\"\n              onChange={e => setDestinationTitle(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"target-date\">Target Date</label>\n            <input type=\"date\" \n              value={targetDate} \n              name=\"target-date\"\n              onChange={e => setTargetDate(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"importance\">Importance</label>\n            <input type=\"number\" \n              value={importance} \n              name=\"importance\"\n              onChange={e => setImportance(e.target.value)} />\n          </div>\n          <button onClick={handleSubmit}>Create Story</button>\n        </>\n      )}\n      <button hidden={page < 2} onClick={handleBack}>Back</button>\n      <button hidden={page > 2} onClick={handleNext}>Next</button>\n    </>\n  );\n}\n \nexport default StoryCreator;","/Users/yuka/Desktop/story_maker/client/src/components/Header.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/NavBar.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MyStory.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/Scroll2.js",[],{"ruleId":"151","replacedBy":"152"},{"ruleId":"153","replacedBy":"154"},{"ruleId":"155","severity":1,"message":"156","line":6,"column":10,"nodeType":"157","messageId":"158","endLine":6,"endColumn":21},{"ruleId":"155","severity":1,"message":"159","line":2,"column":10,"nodeType":"157","messa