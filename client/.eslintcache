or(state => state.story);\n  const activities = useSelector(state => state.activity);\n  const tasks = useSelector(state => state.task);\n  const today = new Date();\n\n  const stringifyDate = date => date.toDateString();\n\n  useEffect(() => {\n    if (stories.length) {\n      for (let story of stories) {\n        dispatch(getDailyTasks(story.id));\n      }\n    }\n    \n    if (!container.current.children.length) {\n      setAllCompleted(true);\n    }\n  }, [stories, dispatch]);\n\n  const onChecked = async (storyId, heroId) => {\n    const checkmarkAnimation = bodymovin.loadAnimation({\n      wrapper: document.querySelector('.svg-container'),\n      animType: 'svg',\n      loop: false,\n      path: '/svg/check-mark.json',\n    });\n\n    checkmarkAnimation.addEventListener('complete', function(){\n      checkmarkAnimation.destroy()\n    });\n\n    const data = await dispatch(completeDailyTask(storyId));\n    if (data.errors) return;\n\n    dispatch(raiseXP(1, heroId));\n    \n    const today = moment();\n    const month = today.month() + 1;\n    const date = today.date();\n    const todaysActivity = activities[heroId][month][date - 1];\n\n    if (todaysActivity) {\n      dispatch(updateActivity(heroId, today));\n    } else {\n      dispatch(createActivity(heroId, today));\n    }\n\n    if (!container.current.children.length) {\n      setAllCompleted(true);\n    }\n  };\n\n  return ( \n    <>\n    <div ref={container} className=\"task__container\">\n      {tasks && (tasks.length ? tasks.map(task => (\n        stringifyDate(today) !== stringifyDate(new Date(task.last_accomplished)) && (\n          <DailyTask key={task.id} task={task} handleClick={onChecked}/>\n        )\n      )) : (\n        <div className=\"hub__content--empty\">\n          <p>There are no daily tasks yet.</p>\n          <p>Daily tasks can be added from story page.</p>\n        </div>\n      ))}\n      {allCompleted ? (<p className=\"task__all-completed\">All done!</p>) : ''}\n    </div>\n    </>\n  );\n}\n \nexport default DailyTaskContainer;","/Users/yuka/Desktop/Projects/story_maker/client/src/config.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/userReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/svg/Frame.js",[],["405","406"],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/progressReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/tokenReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/heroReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/dailyTaskReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/destinationReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/storyReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/memoirReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/activityReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/reducers/monsterReducer.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/svg/Scroll2.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/svg/ActivityScroll.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/svg/DestinationScroll.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/actions/heroAction.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/NavBar.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/InputField.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/store/actions/progressAction.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/StoryHome.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/PastDestination.js",[],"/Users/yuka/Desktop/Projects/story_maker/client/src/components/story/Monster.js",[],"/Users/yuka/Desktop/Proje