odal__button-container\">\n        <button disabled={page < 2} onClick={handleBack}>Back</button>\n        <button disabled={page > 2} onClick={handleNext}>Next</button>\n      </div>\n    </>\n  );\n}\n \nexport default StoryCreator;",{"ruleId":"517","replacedBy":"518"},{"ruleId":"519","replacedBy":"520"},{"ruleId":"521","severity":1,"message":"522","line":7,"column":8,"nodeType":"523","messageId":"524","endLine":7,"endColumn":13},{"ruleId":"521","severity":1,"message":"525","line":8,"column":8,"nodeType":"523","messageId":"524","endLine":8,"endColumn":15},{"ruleId":"521","severity":1,"message":"526","line":10,"column":8,"nodeType":"523","messageId":"524","endLine":10,"endColumn":14},{"ruleId":"521","severity":1,"message":"527","line":11,"column":8,"nodeType":"523","messageId":"524","endLine":11,"endColumn":23},{"ruleId":"528","severity":1,"message":"529","line":30,"column":6,"nodeType":"530","endLine":30,"endColumn":29,"suggestions":"531"},{"ruleId":"521","severity":1,"message":"532","line":1,"column":36,"nodeType":"523","messageId":"524","endLine":1,"endColumn":44},{"ruleId":"521","severity":1,"message":"533","line":2,"column":23,"nodeType":"523","messageId":"524","endLine":2,"endColumn":34},{"ruleId":"521","severity":1,"message":"534","line":3,"column":23,"nodeType":"523","messageId":"524","endLine":3,"endColumn":32},{"ruleId":"521","severity":1,"message":"535","line":3,"column":34,"nodeType":"523","messageId":"524","endLine":3,"endColumn":44},{"ruleId":"521","severity":1,"message":"536","line":4,"column":10,"nodeType":"523","messageId":"524","endLine":4,"endColumn":16},{"ruleId":"521","severity":1,"message":"537","line":5,"column":10,"nodeType":"523","messageId":"524","endLine":5,"endColumn":15},{"ruleId":"521","severity":1,"message":"538","line":5,"column":17,"nodeType":"523","messageId":"524","endLine":5,"endColumn":30},{"ruleId":"521","severity":1,"message":"539","line":6,"column":10,"nodeType":"523","messageId":"524","endLine":6,"endColumn":17},{"ruleId":"5