turn ( \n    <>\n      <h4>Sign Up</h4>\n      <form onSubmit={HandleSignup}>\n        <InputField \n          type=\"text\" \n          placeholder=\"Username\"\n          currentState={username}\n          updateState={setUsername}\n        />\n        <InputField \n          type=\"email\" \n          placeholder=\"Email\"\n          currentState={email}\n          updateState={setEmail}\n        />\n        <InputField \n          type=\"password\" \n          placeholder=\"Password\"\n          currentState={password}\n          updateState={setPassword}\n        />\n        <InputField \n          type=\"password\" \n          placeholder=\"Confirm password\"\n          currentState={confirmPassword}\n          updateState={setConfirmPassword}\n        />\n        <input \n          type='submit' \n          value=\"Submit\" \n        />\n      </form>\n    </>\n  );\n}\n \nexport default SignupForm;","/Users/yuka/Desktop/story_maker/client/src/components/hub/MyHub.js",["238","239","240","241","242"],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/tokenReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/ActivityScroll.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/DestinationScroll.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/HeroContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Hero.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/heroReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/heroAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/StoryContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Story.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/storyAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/storyReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewAdventure.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/ModalContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/StoryCreator.js",["243"],"import React, { useState, useEffect, useRef } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { useHistory } from 'react-router-dom';\nimport { baseUrl } from '../../config';\nimport { LOAD_ERRORS, CLEAR_ERRORS } from '../../store/reducers/errorReducer';\nimport { createStory } from '../../store/actions/storyAction';\n\nconst StoryCreator = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const worldContainer = useRef();\n  const heroContainer = useRef();\n  const heroes = useSelector(state => state.hero);\n\n  const [page, setPage] = useState(1);\n  const [worlds, setWorlds] = useState([]);\n  const [worldId, setWorldId] = useState(null);\n  const [heroId, setHeroId] = useState(null);\n  const [title, setTitle] = useState('');\n  const [destinationTitle, setDestinationTitle] = useState('');\n  const [targetDate, setTargetDate] = useState('');\n  const [importance, setImportance] = useState('');\n  const [pageTitle, setPageTitle] = useState('');\n\n  useEffect(() => {\n    (async () => {\n      if (!worlds.length) {\n        const res = await fetch(`${baseUrl}/worlds`);\n        const data = await res.json();\n        setWorlds(data.worlds);\n      }\n    })();\n  }, [worlds]);\n\n  useEffect(() => {\n    if (page === 2) {\n      if (heroContainer.current.children.length && heroId) {\n        heroContainer.current.childNodes.forEach(child => child.classList.remove('selected'));\n        heroContainer.current.childNodes[heroId - 1].classList.add('selected');\n      }\n    }\n  }, [heroId, page]);\n\n  useEffect(() => {\n    if (page === 1) {\n      setPageTitle('Set Your Story Name');\n    } else if (page === 2) {\n      setPageTitle('Select Your Hero');\n    } else {\n      setPageTitle('Set Your Destination');\n    }\n  }, [page])\n\n  const handleSubmit = async e => {\n    if ( !heroId || !title || !destinationTitle || !targetDate || !importance) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['There is at least one field with missing value.']\n      });\n    }\n    const data = await dispatch(createStory({ worldId, heroId, title, \n                                destinationTitle, targetDate, importance  }));\n\n    if(!data.errors) {\n      history.push(`/stories/${data.stories[data.stories.length - 1].id}`);\n    }\n  }\n\n  const handleNext = () => {\n    if ((page === 1 && !title) || (page === 2 && !heroId)) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['Please select/fill out to proceed.']\n      });\n    }\n    dispatch({ type: CLEAR_ERRORS });\n    setPage(page + 1);\n  }\n  \n  const handleBack = () => {\n    setPage(page - 1);\n  }\n\n  return ( \n    <>\n      <h2>{pageTitle}</h2>\n      {page === 1 && (\n        <div>\n            <label for=\"title\">Story Title</label>\n            <input type=\"text\" \n              value={title} \n              name=\"title\"\n              onChange={e => setTitle(e.target.value)} />\n          </div>\n      )}\n      {page === 2 && (\n        <div className=\"modal__page-container\" ref={heroContainer}> \n          {heroes.length && heroes.map(hero => (\n            <div className=\"hero\" onClick={() => {\n              setHeroId(hero.id);\n              setWorldId(hero.worldId) }}>\n              <img src={hero.image} alt={hero.id} />\n              <p>{hero.name}</p>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 3 && (\n        <>\n          <div>\n            <label for=\"destination-title\">Final Goal</label>\n            <input type=\"text\" \n              value={destinationTitle} \n              name=\"destination-title\"\n              onChange={e => setDestinationTitle(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"target-date\">Target Date</label>\n            <input type=\"date\" \n              value={targetDate} \n              name=\"target-date\"\n              onChange={e => setTargetDate(e.target.value)} />\n          </div>\n          <div>\n            <label for=\"importance\">Importance</label>\n            <input type=\"number\" \n              value={importance} \n              name=\"importance\"\n              onChange={e => setImportance(e.target.value)} />\n          </div>\n          <button onClick={handleSubmit}>Create Story</button>\n        </>\n      )}\n      <button hidden={page < 2} onClick={handleBack}>Back</button>\n      <button hidden={page > 2} onClick={handleNext}>Next</button>\n    </>\n  );\n}\n \nexport default StoryCreator;","/Users/yuka/Desktop/story_maker/client/src/components/Header.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/NavBar.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MyStory.js",["244"],"import React, { useState, useEffect } from 'react';\nimport { useDispatch } from 'react-redux';\nimport { useParams } from 'react-router-dom';\nimport Scroll2 from '../svg/Scroll2';\nimport NewDestination from './NewDestination';\nimport DestinationContainer from '../DestinationContainer';\nimport SideMenuField from './SideMenuField';\nimport ProgressContainer from './ProgressContainer';\nimport MemoirContainer from './MemoirContainer';\nimport { getStory } from '../../store/actions/storyAction';\n\nconst MyStory = () => {\n  const dispatch = useDispatch();\n  const { id } = useParams();\n  const [currentDisplay, setCurrentDisplay] = useState(<DestinationContainer />);\n  const [currentTitle, setCurrentTitle] = useState(\"Upcoming destinations\");\n\n  useEffect(() => {\n    dispatch(getStory(id));\n  }, []);\n\n  return (  \n    <div className=\"mystory\">\n       <div className=\"mystory__progress\">\n         <ProgressContainer />\n       </div>\n       <div className=\"mystory__main\">\n         <Scroll2 text={currentTitle} className=\"mystory__scroll\" \n                  width=\"40rem\" fontSize=\".5rem\" />\n         <div className=\"mystory__container\">\n          {currentDisplay}\n         </div>\n       </div>\n       <div className=\"mystory__sidebar-left\">\n       <SideMenuField>\n        <div onClick={() => {\n          setCurrentDisplay(<DestinationContainer />);\n          setCurrentTitle(\"Upcoming Destinations\");\n        }}>\n          View Upcoming Destinations\n        </div>\n       </SideMenuField>\n       <SideMenuField>\n       <div onClick={() => {\n          setCurrentDisplay(<MemoirContainer />);\n          setCurrentTitle(\"View Memoirs\");\n        }}>\n          Memoirs\n        </div>\n       </SideMenuField>\n\n       </div>\n       <div className=\"mystory__sidebar-right\">\n        <NewDestination />\n       </div>\n    </div>\n  );\n}\n \nexport default MyStory;","/Users/yuka/Desktop/story_maker/client/src/components/svg/Scroll2.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewDestination.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewStoryModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/DestinationContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/Destination.js",["245","246","247"],"import React from 'react';\n\nconst Destination = ({...props}) => {\n  const { id, parent_destination_id: parentDestinationId,\n        title, description, target_date: targetDate, importance } = props;\n  return ( \n    <div className=\"destination\">\n      <p className=\"destination__title\">{title}</p>\n      <p className=\"destination__description\">{description}</p>\n      <p className=\"destination__target-date\">{targetDate}</p>\n    </div>\n  );\n}\n \nexport default Destination;","/Users/yuka/Desktop/story_maker/client/src/store/actions/destinationActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/destinationReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/utils/dateFormatter.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/SideMenuField.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewMemoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/ProgressContainer.js",["248","249"],"import React, { useEffect, useState, useRef } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Canvas } from 'react-three-fiber';\nimport { Physics } from \"@react-three/cannon\";\nimport { Sky } from 'drei';\nimport { Vector3 } from 'three';\n\nimport Plane from './three/Plane';\nimport Node from './three/Node';\nimport Camera from './three/Camera';\nimport Hero from './three/Hero';\n\nimport { getHero } from '../../store/actions/heroAction';\n\nconst ProgressContainer = () => {\n  const dispatch = useDispatch();\n  const heroId = useSelector(state => state.story[0] ? \n                  state.story[0].hero_id : null);\n  const hero = useSelector(state => state.hero[0]);\n  const destinations = useSelector(state => state.destination);\n  const [hovered, setHover] = useState(false);\n  const [active, setActive] = useState(null);\n  const container = useRef();\n  let x = -10;\n\n  useEffect(() => {\n    dispatch(getHero(heroId));\n  }, [heroId, dispatch]);\n\n  return ( \n    <div ref={container} className=\"progress__container\">\n      <Canvas\n        onCreated={({ gl }) => gl.setClearColor('lightblue')}\n        colorManagement>\n        <Camera position={[0, 1, 20]}\n                reference={container.current}/>\n        <ambientLight intensity={0.5} />\n        <spotLight position={[10, 15, 10]} angle={3} />\n        <Sky sunPosition={new Vector3(10, 20, 100)}/>\n        <Physics\n          velocity={0}\n          gravity={[0, -5, 0]} >\n          {destinations.map(destination => {\n            x += 3;\n            return <Node\n                      destination={destination} \n                      active={active}\n                      setActive={setActive}\n                      position={[x, 5, 0]} />\n          })}\n          <Hero hero={hero}\n                active={active}\n                setActive={setActive}\n                position={[-10, 5, 0]} />\n          <Plane />\n        </Physics>\n        {/* {active && (\n          <Html>\n            <div>Hiii</div>\n          </Html>\n        )} */}\n      </Canvas>\n    </div>\n  );\n}\n\nexport default ProgressContainer;","/Users/yuka/Desktop/story_maker/client/src/store/reducers/memoirReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/memoirActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MemoirContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/Memoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewHeroModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/HeroCreator.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/three/Plane.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/three/Node.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/three/Camera.js",["250"],"import React, { useEffect, useRef } from 'react';\nimport { a } from '@react-spring/three';\nimport { useFrame, useThree } from 'react-three-fiber';\nimport CanvasScroll from './CanvasScroll';\n\nconst Camera = (props) => {\n  const [x] = CanvasScroll([1, 100], { domTarget: props.reference });\n  const ref = useRef()\n  const { setDefaultCamera } = useThree()\n  useEffect(() => void setDefaultCamera(ref.current), [])\n  useFrame(() => ref.current.updateMatrixWorld())\n\n  return ( \n    <a.perspectiveCamera ref={ref} {...props} \n          fov=\"10\" position-x={x.to((x) => (x / 500) * 25)}/>\n   );\n}\n \nexport default Camera;","/Users/yuka/Desktop/story_maker/client/src/components/story/three/CanvasScroll.js",["251"],"import React, { useEffect, useCallback } from 'react';\nimport { useGesture } from 'react-use-gesture';\nimport { useSpring, config } from '@react-spring/core';\nimport clamp from \"lodash/clamp\";\n\nconst CanvasScroll = (bounds, props) => {\n  const [{ x }, set] = useSpring(() => ({ x: 0, config: config.slow }));\n  const fn = useCallback(\n    ({ xy: [cx, ], previous: [, px], memo = x.get() }) => {\n      const newX = clamp(memo + cx - px, ...bounds);\n      set({ x: newX });\n      return newX;\n    },\n    [bounds, x, set]\n  );\n  const bind = useGesture({ onWheel: fn, onDrag: fn }, props);\n  useEffect(() => props && props.domTarget && bind(), [props, bind]);\n  return [x, bind];\n}\n \nexport default CanvasScroll;","/Users/yuka/Desktop/story_maker/client/src/components/story/three/Hero.js",["252"],"import React, { Suspense, useEffect } from 'react';\nimport { useBox } from \"@react-three/cannon\";\nimport { useTexture } from '@react-three/drei';\n\nconst Hero = ({hero, active, setActive, ...props}) => {\n  const [ref] = useBox(() => ({ mass: 1, ...props }));\n  // const texture = useTexture('https://story-maker-app.s3.amazonaws.com/bungalo.png')\n  const texture = useTexture(hero ? hero.image : '')\n\n  return ( \n    <Suspense fallback={null}>\n      <mesh \n        ref={ref}\n        >\n        <planeBufferGeometry attach=\"geometry\" />\n        <meshStandardMaterial map={texture} />\n      </mesh>\n    </Suspense>\n  );\n}\n \nexport default Hero;","/Users/yuka/Desktop/story_maker/client/src/components/hub/ActivityContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Activity.js",["253","254"],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/activityReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/activityAction.js",[],{"ruleId":"255","replacedBy":"256"},{"ruleId":"257","replacedBy":"258"},{"ruleId":"259","severity":1,"message":"260","line":6,"column":10,"nodeType":"261","messageId":"262","endLine":6,"endColumn":21},{"ruleId":"259","severity":1,"message":"263","line":1,"column":17,"nodeType":"261","messageId":"262","endLi