tory } from '../../store/actions/storyAction';\n\nconst StoryCreator = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const worldContainer = useRef();\n  const heroContainer = useRef();\n  const heroes = useSelector(state => state.hero);\n\n  const [page, setPage] = useState(1);\n  const [worlds, setWorlds] = useState([]);\n  const [worldId, setWorldId] = useState(null);\n  const [heroId, setHeroId] = useState(null);\n  const [title, setTitle] = useState('');\n  const [destinationTitle, setDestinationTitle] = useState('');\n  const [targetDate, setTargetDate] = useState('');\n  const [importance, setImportance] = useState('');\n  const [pageTitle, setPageTitle] = useState('');\n\n  useEffect(() => {\n    (async () => {\n      if (!worlds.length) {\n        const res = await fetch(`${baseUrl}/worlds`);\n        const data = await res.json();\n        setWorlds(data.worlds);\n      }\n    })();\n  }, [worlds]);\n\n  useEffect(() => {\n    if (page === 2) {\n      if (heroContainer.current.children.length && heroId) {\n        heroContainer.current.childNodes.forEach(child => child.classList.remove('selected'));\n        heroContainer.current.childNodes[heroId - 1].classList.add('selected');\n      }\n    }\n  }, [heroId, page]);\n\n  useEffect(() => {\n    if (page === 1) {\n      setPageTitle('Set Your Story Name');\n    } else if (page === 2) {\n      setPageTitle('Select Your Hero');\n    } else {\n      setPageTitle('Set Your Destination');\n    }\n  }, [page])\n\n  const handleSubmit = async e => {\n    if ( !heroId || !title || !destinationTitle || !targetDate || !importance) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['There is at least one field with missing value.']\n      });\n    }\n    const data = await dispatch(createStory({ worldId, heroId, title, \n                                destinationTitle, targetDate, importance  }));\n\n    if(!data.errors) {\n      history.push(`/stories/${data.stories[data.stories.length - 1].id}`);\n    }\n  }\n\n  const handleNext = () => {\n    if ((page === 1 && !title) || (page === 2 && !heroId)) {\n      return dispatch({\n        type: LOAD_ERRORS,\n        errors: ['Please select/fill out to proceed.']\n      });\n    }\n    dispatch({ type: CLEAR_ERRORS });\n    setPage(page + 1);\n  }\n  \n  const handleBack = () => {\n    setPage(page - 1);\n  }\n\n  return ( \n    <>\n      <h2>{pageTitle}</h2>\n      {page === 1 && (\n        <div className=\"modal__field\">\n            <label for=\"title\">Story Title</label>\n            <input type=\"text\" \n              value={title} \n              name=\"title\"\n              onChange={e => setTitle(e.target.value)} />\n          </div>\n      )}\n      {page === 2 && (\n        <div className=\"modal__page-container\" ref={heroContainer}> \n          {heroes.length && heroes.map(hero => (\n            <div className=\"hero\" onClick={() => {\n              setHeroId(hero.id);\n              setWorldId(hero.worldId) }}>\n              <img src={hero.image} alt={hero.id} />\n              <p>{hero.name}</p>\n            </div>\n          ))}\n        </div>\n      )}\n      {page === 3 && (\n        <>\n          <div className=\"modal__field\">\n            <label for=\"destination-title\">Final Goal</label>\n            <input type=\"text\" \n              value={destinationTitle} \n              name=\"destination-title\"\n              onChange={e => setDestinationTitle(e.target.value)} />\n          </div>\n          <div className=\"modal__field\">\n            <label for=\"target-date\">Target Date</label>\n            <input type=\"date\" \n              value={targetDate} \n              name=\"target-date\"\n              onChange={e => setTargetDate(e.target.value)} />\n          </div>\n          <div className=\"modal__field\">\n            <label for=\"importance\">Importance</label>\n            <input type=\"number\" \n              value={importance} \n              name=\"importance\"\n              onChange={e => setImportance(e.target.value)} />\n          </div>\n          <button onClick={handleSubmit}>Create Story</button>\n        </>\n      )}\n      <div className=\"modal__button-container\">\n        <button hidden={page < 2} onClick={handleBack}>Back</button>\n        <button hidden={page > 2} onClick={handleNext}>Next</button>\n      </div>\n    </>\n  );\n}\n \nexport default StoryCreator;","/Users/yuka/Desktop/story_maker/client/src/components/NavBar.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MyStory.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/svg/Scroll2.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewDestination.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewStoryModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/DestinationContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/Destination.js",["294","295","296"],"/Users/yuka/Desktop/story_maker/client/src/store/actions/destinationActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/destinationReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/utils/dateFormatter.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/SideMenuField.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/NewMemoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/memoirReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/memoirActions.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/MemoirContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/story/Memoir.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/NewHeroModal.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/HeroCreator.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/ActivityContainer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/Activity.js",["297","298","299","300"],"/Users/yuka/Desktop/story_maker/client/src/store/reducers/activityReducer.js",[],"/Users/yuka/Desktop/story_maker/client/src/store/actions/activityAction.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/HeaderContainer.js",["301","302","303","304","305"],"/Users/yuka/Desktop/story_maker/client/src/components/three/Hero.js",["306"],"import React, { Suspense, useEffect } from 'react';\nimport { useBox } from \"@react-three/cannon\";\nimport { useTexture } from '@react-three/drei';\n\nconst Hero = ({hero, active, setActive, ...props}) => {\n  const [ref] = useBox(() => ({ mass: 1, ...props }));\n  // const texture = useTexture('https://story-maker-app.s3.amazonaws.com/bungalo.png')\n  const texture = useTexture(hero ? hero.image : '')\n\n  return ( \n    <Suspense fallback={null}>\n      <mesh \n        ref={ref}\n        >\n        <planeBufferGeometry attach=\"geometry\" />\n        <meshStandardMaterial map={texture} />\n      </mesh>\n    </Suspense>\n  );\n}\n \nexport default Hero;","/Users/yuka/Desktop/story_maker/client/src/components/three/Camera.js",["307"],"import React, { useEffect, useRef } from 'react';\nimport { a } from '@react-spring/three';\nimport { useFrame, useThree } from 'react-three-fiber';\nimport CanvasScroll from './CanvasScroll';\n\nconst Camera = (props) => {\n  const [x] = CanvasScroll([1, 100], { domTarget: props.reference });\n  const ref = useRef()\n  const { setDefaultCamera } = useThree()\n  useEffect(() => void setDefaultCamera(ref.current), [])\n  useFrame(() => ref.current.updateMatrixWorld())\n\n  return ( \n    <a.perspectiveCamera ref={ref} {...props} \n          fov=\"10\" position-x={x.to((x) => (x / 500) * 25)}/>\n   );\n}\n \nexport default Camera;","/Users/yuka/Desktop/story_maker/client/src/components/three/CanvasScroll.js",["308"],"import React, { useEffect, useCallback } from 'react';\nimport { useGesture } from 'react-use-gesture';\nimport { useSpring, config } from '@react-spring/core';\nimport clamp from \"lodash/clamp\";\n\nconst CanvasScroll = (bounds, props) => {\n  const [{ x }, set] = useSpring(() => ({ x: 0, config: config.slow }));\n  const fn = useCallback(\n    ({ xy: [cx, ], previous: [, px], memo = x.get() }) => {\n      const newX = clamp(memo + cx - px, ...bounds);\n      set({ x: newX });\n      return newX;\n    },\n    [bounds, x, set]\n  );\n  const bind = useGesture({ onWheel: fn, onDrag: fn }, props);\n  useEffect(() => props && props.domTarget && bind(), [props, bind]);\n  return [x, bind];\n}\n \nexport default CanvasScroll;","/Users/yuka/Desktop/story_maker/client/src/components/three/Plane.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/three/Node.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/Footer.js",[],"/Users/yuka/Desktop/story_maker/client/src/components/hub/HubHeader.js",["309"],"import React, { Suspense } from 'react';\n\nimport { useLocation } from 'react-router-dom';\nimport { Canvas } from 'react-three-fiber';\nimport { Stars, OrbitControls } from 'drei';\nimport { useGLTF, Loader } from '@react-three/drei';\n\nconst Tavern = () => {\n  const gltf = useGLTF('/scene.gltf', true);\n\n  return (\n    <group position={[0, -10, 0]} scale={[.1, .1, .1]}>\n      <primitive object={gltf.scene} dispose={null}></primitive>\n    </group>\n  )\n}\n\nconst HubHeader = () => {\n  return ( \n    <header className='header'>\n      <Canvas>\n        <Stars \n          radius={200}\n          depth={500}\n          count={3000} />\n      <OrbitControls\n          enablePan={false}\n          enableZoom={false}\n          enableDamping\n          dampingFactor={0.5}\n          maxPolarAngle={Math.PI / 2}\n          minPolarAngle={Math.PI / 2} />\n        <ambientLight intensity={0.5} />\n        <Suspense fallback={null}>\n          <Tavern />\n        </Suspense>\n      </Canvas>\n      <Loader />\n    </header>\n  );\n}\n \nexport default HubHeader